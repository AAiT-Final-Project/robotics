<launch>
    <env name="GAZEBO_MODEL_PATH" value="$(find arm_description)/model"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find arm_gazebo)/worlds/arm.world" />
    </include>
    <node name="arm_sdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find arm_description)/model/model.sdf -sdf -model arm">
    </node>
</launch>